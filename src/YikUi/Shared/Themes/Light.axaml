<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush Color="#828e9f" x:Key="ForegroundColor" />
            <SolidColorBrush Color="#e7eaed" x:Key="LineColor" />
            <Color x:Key="BackgroundColor">
                #f8f9fa
            </Color>
            <SolidColorBrush
                Color="#e7eaed"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="SelectionNormal" />
            <SolidColorBrush
                Color="#f5f5f5"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="PanelBackgroundColor" />
            <SolidColorBrush
                Color="#eef0f2"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="PanelBorderBrush" />
            <SolidColorBrush
                Color="#ffffff"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="PopupBackgroundColor" />
            <SolidColorBrush
                Color="#eaedf0"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="PopupBorderBrush" />

            <SolidColorBrush
                Color="#dadddf"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="SelectedTabBackgroundColor" />
            <SolidColorBrush
                Color="#e8e8e8"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="HoverTabBackgroundColor" />
            <SolidColorBrush
                Color="#f1f1f1"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="TabBackgroundColor" />
            <SolidColorBrush
                Color="#a1a1a1"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="TabTextColor" />
            <SolidColorBrush
                Color="#5c6773"
                Opacity="{DynamicResource BackGroundOpacity}"
                x:Key="SelectedTabTextColor" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>